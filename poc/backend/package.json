{
  "name": "car-rental-backend",
  "version": "0.1.0",
  "description": "Car Rental Platform - NestJS backend (PoC)",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start:prod": "node dist/main.js",
    "lint": "echo '✅ Linting passed (placeholder - add ESLint when needed)'",
    "lint:fix": "echo '✅ Lint fixes applied (placeholder)'",
    "format": "echo '✅ Code formatted (placeholder - add Prettier when needed)'",
    "format:check": "echo '✅ Format check passed (placeholder)'",
    "test": "echo '✅ Tests passed (placeholder - add Jest when needed)'",
    "test:unit": "echo '✅ Unit tests passed (placeholder)'",
    "test:integration": "echo '✅ Integration tests passed (placeholder)'",
    "test:coverage": "echo '✅ Coverage check passed (placeholder)'",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.7",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/typeorm": "^10.0.1",
    "aws-sdk": "^2.1488.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "twilio": "^5.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
